[run]
source = .
omit = 
    */migrations/*
    */venv/*
    */env/*
    */virtualenv/*
    .cache/*
    .git/*
    .local/*
    .pythonlibs/*
    .upm/*
    manage.py
    config/wsgi.py
    config/asgi.py
    */settings.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
directory = htmlcov